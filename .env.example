# 数据库配置 - 请替换为你的Supabase配置
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Redis配置
REDIS_URL=redis://localhost:6379

# JWT密钥 - 请替换为强密码
JWT_SECRET=your-super-secret-jwt-key-here

# 第三方平台API密钥 - 请替换为你的实际密钥
DOUYIN_CLIENT_ID=your-douyin-client-id
DOUYIN_CLIENT_SECRET=your-douyin-client-secret
KUAISHOU_CLIENT_ID=your-kuaishou-client-id
KUAISHOU_CLIENT_SECRET=your-kuaishou-client-secret
XIAOHONGSHU_CLIENT_ID=your-xiaohongshu-client-id
XIAOHONGSHU_CLIENT_SECRET=your-xiaohongshu-client-secret

# 文件存储配置
STORAGE_TYPE=supabase
SUPABASE_STORAGE_BUCKET=videos

# 服务器配置
PORT=3000
NODE_ENV=development

# 视频处理配置
MAX_VIDEO_SIZE=500MB
MAX_CONCURRENT_DOWNLOADS=3
VIDEO_PROCESSING_TIMEOUT=300000